<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
                android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
       android:layout_height="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_sysbmol_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="--"
                android:textColor="@color/midle_gray"
                android:textSize="14dp"/>

            <TextView

                android:id="@+id/tv_shoushu"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="0手"
                android:textColor="@color/midle_gray"
                android:layout_marginLeft="30dp"
                android:textSize="14dp"/>

        </LinearLayout>



        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_loss"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="--"
                android:textColor="@color/light_green"
                android:textSize="14sp"/>

            <TextView
                android:id="@+id/tv_user_make"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_marginLeft="24dp"
                android:text="用户操作"
                android:textColor="#12AEFF"
                android:textSize="@dimen/dp14"/>

        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="6dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="盈亏"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:textColor="@color/light_gray"
                android:textSize="12sp"/>

            <TextView
                android:id="@+id/tv_yingkui_num"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:text="--"
                android:textColor="@android:color/holo_red_light"
                android:layout_marginLeft="3dp"
                android:textSize="12sp"/>

        </LinearLayout>






        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="单号"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:textColor="@color/light_gray"
                android:textSize="12sp"/>

            <TextView

                android:id="@+id/tv_oddNumbers"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"

                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:layout_marginLeft="4dp"
                android:text="--"
                android:textColor="@color/light_gray"
                android:textSize="12dp"/>

        </LinearLayout>




    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="6dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="触发止盈"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:textColor="@color/light_gray"
                android:textSize="12dp"/>

            <TextView
                android:id="@+id/tv_stop_Profit"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"

                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:text="--"
                android:textColor="@color/light_gray"
                android:layout_marginLeft="3dp"
                android:textSize="12dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

                <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="触发止损"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:textColor="@color/light_gray"
                android:textSize="12sp"/>

            <TextView
                android:id="@+id/tv_stop_loss"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"

                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:layout_marginLeft="4dp"
                android:text="--"
                android:textColor="@color/light_gray"
                android:textSize="12dp"/>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="6dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="开仓 "
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:textColor="@color/light_gray"
                android:textSize="12dp"/>

            <TextView
                android:id="@+id/tv_open_position"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"

                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:text="--"
                android:textColor="@color/light_gray"
                android:layout_marginLeft="3dp"
                android:textSize="12dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="平仓 "

                android:gravity="center_vertical"
                android:layout_gravity="center_vertical"
                android:textColor="@color/light_gray"
                android:textSize="12sp"/>

            <TextView
                android:id="@+id/tv_close_position"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_gravity="center_vertical"

                android:layout_marginLeft="4dp"
                android:text="--"
                android:textColor="@color/light_gray"
                android:textSize="12dp"/>

        </LinearLayout>

    </LinearLayout>




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="6dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="平仓时间  "
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:textColor="@color/light_gray"
                android:textSize="12dp"/>

            <TextView
                 android:id="@+id/tv_close_position_time"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"

                 android:layout_gravity="center_vertical"
                 android:gravity="center_vertical"
                android:text="--"
                android:textColor="@color/light_gray"
                android:layout_marginLeft="3dp"
                android:textSize="12dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="交易费用 "
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:textColor="@color/light_gray"
                android:textSize="12sp"/>

            <TextView
                android:id="@+id/tv_trade_free"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"

                android:layout_marginLeft="4dp"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:text="--"
                android:textColor="@color/light_gray"
                android:textSize="12dp"/>

        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="6dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="开仓时间 "
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:textColor="@color/light_gray"
                android:textSize="12dp"/>

            <TextView
               android:id="@+id/tv_open_position_time"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"

               android:layout_gravity="center_vertical"
               android:gravity="center_vertical"
                android:text="--"
                android:textColor="@color/light_gray"
                android:layout_marginLeft="3dp"
                android:textSize="12dp"/>

        </LinearLayout>

    </LinearLayout>






</LinearLayout>
